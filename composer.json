{
    "name": "solution-forest/filament-access-management",
    "description": "filament-access-management",
    "keywords": [
        "Solution Forest",
        "laravel",
        "filament-access-management"
    ],
    "homepage": "https://github.com/solutionforest/filament-access-management",
    "support": {
        "issues": "https://github.com/solutionforest/filament-access-management/issues",
        "source": "https://github.com/solutionforest/filament-access-management"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carly",
            "email": "info@solutionforest.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "guava/filament-icon-picker": "^2.0",
        "solution-forest/filament-tree": "^2.0.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-permission": "^5.5|^6.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "SolutionForest\\FilamentAccessManagement\\": "src",
            "SolutionForest\\FilamentAccessManagement\\Database\\Factories\\": "database/factories",
            "SolutionForest\\FilamentAccessManagement\\Database\\Seeders\\": "database/seeders"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SolutionForest\\FilamentAccessManagement\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:pest": "vendor/bin/pest --parallel",
        "test:phpstan": "vendor/bin/phpstan analyse",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SolutionForest\\FilamentAccessManagement\\FilamentAccessManagementServiceProvider"
            ],
            "aliases": {
                "FilamentAccessManagement": "SolutionForest\\FilamentAccessManagement\\Facades\\FilamentAccessManagement"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/raphaelcangucu/filament-tree.git"
        }
    ]
}